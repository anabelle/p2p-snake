{
  "name": "multiplayer-snake-game",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^18.0.9",
    "@types/react-modal": "^3.16.3",
    "netplayjs": "^0.4.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.3",
    "react-scripts": "5.0.1",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stream-browserify": "^3.0.0",
    "tsx": "^4.19.3",
    "typescript": "^4.9.3",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ci": "react-app-rewired test --ci --detectOpenHandles",
    "eject": "react-scripts eject",
    "preview": "vite preview",
    "start:server": "tsx server/index.ts",
    "start:dev": "cross-env REACT_APP_SIGNALING_SERVER_URL=http://localhost:3001 concurrently \"npm run start:server\" \"npm run start\"",
    "heroku-postbuild": "npm run build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "lint": "eslint . --fix --max-warnings=0",
    "format": "prettier . --write",
    "strip-comments": "node strip-comments.js",
    "verify": "concurrently \"npm run cypress:run\" \"npm test -- --watchAll=false --silent\" \"npm run build\" \"npm run lint\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "node strip-comments.js"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@types/jest": "^27.5.2",
    "@types/simple-peer": "^9.11.8",
    "@types/uuid": "^9.0.8",
    "buffer": "^6.0.3",
    "canvas": "^2.11.2",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "glob": "^10.4.5",
    "husky": "^8.0.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "react-app-rewired": "^2.2.1",
    "semantic-release": "^22.0.12",
    "strip-comments": "^2.0.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/reportWebVitals.ts",
      ".*types\\.ts$",
      "src/index.tsx"
    ],
    "watchPlugins": []
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": ["package.json", "package-lock.json", "CHANGELOG.md"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\\n\\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
